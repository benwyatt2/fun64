
cd `dirname $0`
cd bin

echo "creating new orphan branch"

git checkout --orphan bintmp

echo "copying exe from gamecake/exe"

cp -r ../../gamecake/exe .
rm exe/pagecake*

echo "adding files to new branch"

git add . && git commit -m"bin bump"

echo "renaming/replacing branch"

git branch -D bin
git branch -m bin

echo "force pushing"

git push --set-upstream origin -f bin

echo "updating reference to new bin"

cd ..
git commit -a -m"bin bump"
git push



